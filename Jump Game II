
//动归
public class Solution {
    public int jump(int[] nums) {
        int n = nums.length;
        int[] f = new int [n];
        f[0]=0;
        for(int i=0;i<n;i++){           
            for(int j = 0;j<i ; j++){
                if(j+nums[j]>=i){
                    f[i]=f[j]+1;
                    break;                   
                }                   
            }
        }
        return f[n-1];
    }
}
